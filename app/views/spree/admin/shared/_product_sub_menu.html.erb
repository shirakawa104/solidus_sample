<ul id="sub_nav" class="admin-subnav" data-hook="admin_product_sub_tabs">
  <% if can? :admin, Spree::Product %>
    <%= tab :products, label: Spree.t('products'), match_path: '/products' %>
  <% end %>
  <% if can? :admin, Spree::OptionType %>
    <%= tab :option_types, label: Spree.t('option_types'), tch_path: '/option_types' %>
  <% end %>
  <% if can? :admin, Spree::Property %>
    <%= tab :properties, label: Spree.t('properties') %>
  <% end %>
  <% if can? :admin, Spree::Prototype %>
    <%= tab :prototypes, label: Spree.t('prototypes') %>
  <% end %>
  <% if can? :admin, Spree::Taxonomy %>
    <%= tab :taxonomies, label: Spree.t('taxonomies') %>
  <% end %>
  <% if can? :admin, Spree::Taxon %>
    <%= tab :taxons, label: Spree.t('taxons') %>
  <% end %>
</ul>
